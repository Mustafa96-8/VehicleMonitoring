@model GPSReading
@{

    ViewData["Title"] = "GPS датчик";
}
<form method="get">
    <div class="container border p-3 mt-4">
        <div class="p-3 pb-1">
            <label asp-for="GPSDataId" class="p-0"></label>:<p>@Model.GPSDataId</p>
        </div>
        <div class="p-3 pb-1">
            <label asp-for="latitude" class="p-0"></label>:<p>@Model.latitude</p>
        </div>
        <div class="p-3 pb-1">
            <label asp-for="longitude" class="p-0"></label>:<p>@Model.longitude</p>
        </div>
        <div class="p-3 pb-1">
            <label asp-for="altitude" class="p-0"></label>:<p>@Model.altitude</p>
        </div>
        <div class="p-3 pb-1">
            <label asp-for="speed" class="p-0"></label>:<p>@Model.speed</p>
        </div>
        <div class="p-3 pb-1">
            <label asp-for="heading" class="p-0"></label>:<p>@Model.heading</p>
        </div>
        <div class="p-3 pb-1">
            <label asp-for="CreationTime" class="p-0"></label>:<p>@Model.CreationTime</p>
        </div>

        <div class="row">
            <div class="w-75 btn-group" role="group">
                <a asp-controller="GPSReading" asp-action="Index" class="btn btn-outline-primary mx-1 rounded-2">
                    <i class="bi bi-box-arrow-in-left"></i> Back to list
                </a>
                <a asp-controller="GPSReading" asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-outline-primary mx-1 rounded-2">
                    <i class="bi bi-pencil-square"></i> Edit
                </a>
                <a asp-controller="GPSReading" asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-outline-danger mx-1 rounded-2">
                    <i class="bi bi-trash3"></i> Delete
                </a>
            </div>
        </div>
    </div>
</form>
